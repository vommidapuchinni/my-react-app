{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/home/ubuntu/my-react-app/logs/*.log",
            "log_group_name": "/production/react-app",
            "log_stream_name": "{instance_id}",
            "create_group": true
          }
        ]
      }
    }
  }
}

